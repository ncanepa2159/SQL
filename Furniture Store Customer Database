--Create a Furniture Store Database--

CREATE TABLE January_Sales
(id INTEGER PRIMARY KEY AUTOINCREMENT, 
transaction_date TEXT,
product_type TEXT, 
product_price INTEGER, 
payment_type TEXT, 
city_of_transaction TEXT, 
state_of_transaction TEXT,
customer_name TEXT, 
customer_phone_number TEXT,
current_rewards_member TEXT,
rewards_member_number TEXT,
customer_email TEXT);

INSERT INTO January_Sales

(transaction_date, 
product_type, 
product_price, 
payment_type, 
city_of_transaction, 
state_of_transaction, 
customer_name, 
customer_phone_number,
current_rewards_member,
rewards_member_number,
customer_email) 

