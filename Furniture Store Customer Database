--Display the database ordered by price where sales are greater than $350 and by when sales occured.
SELECT * FROM January_Sales WHERE product_price > 350 ORDER BY transaction_date;

--Display the database ordered by customers who bought beds and are current reward members.
SELECT * FROM January_Sales where product_type='Bed' AND current_rewards_member = 'Yes';

--Display the database ordered by customers who are not current reward members and made purchases greater than $750.
SELECT * FROM January_Sales where current_rewards_member = 'No' AND product_price > 750;

--Display the database ordered by customers who are not current reward members and made purchases in various categories to determine which customers should be encouraged to sign up based on lowest to biggest spenders. 
SELECT  customer_email, customer_name, 
customer_phone_number,
CASE
    WHEN product_price <= 750 THEN 'Medium Spender'
    WHEN product_price <= 1200 THEN 'High Spender'
    WHEN product_price <= 3500 THEN 'Big Spender'
    AND current_rewards_member = 'No'
  END AS Target_Customers_To_Sign_Up_For_Rewards_Program
FROM    January_Sales;
